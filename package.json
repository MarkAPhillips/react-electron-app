{
  "name": "react-electron-app",
  "version": "1.0.0",
  "description": "Electron react boiler plate application",
  "main": "dist/main.js",
  "scripts": {
    "build:ui": "webpack --mode development --config webpack.ui.config.js",
    "build:electron": "webpack --mode development",
    "build:app": "electron-packager . --overwrite",
    "start": "npm run build:ui && npm run build:electron && electron ./dist/main.js"
  },
  "author": "Mark Phillips",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-transform-arrow-functions": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-react": "^7.8.3",
    "@babel/preset-typescript": "^7.8.3",
    "@types/react": "^16.9.19",
    "@types/react-dom": "^16.9.5",
    "@typescript-eslint/eslint-plugin": "^2.19.0",
    "@typescript-eslint/parser": "^2.19.0",
    "babel-loader": "^8.0.6",
    "electron": "^8.5.2",
    "eslint": "^6.8.0",
    "html-webpack-plugin": "^3.2.0",
    "prettier": "^1.19.1",
    "typescript": "^3.7.5",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10"
  },
  "dependencies": {
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "styled-components": "^5.0.1"
  }
}
